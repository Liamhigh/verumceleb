# Verum Omnis Environment Variables
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control

# OpenAI API Key (for AI verification and chat)
OPENAI_API_KEY=sk-your-openai-key-here

# Anthropic API Key (for Claude verification)
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here

# DeepSeek API Key (for DeepSeek verification)
DEEPSEEK_API_KEY=your-deepseek-key-here

# Firebase Project ID
FIREBASE_PROJECT_ID=verumdone

# Optional: Skip immutable verification during development
# SKIP_IMMUTABLE_VERIFY=1
